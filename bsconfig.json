{  
  "name": "rescript-project-template",
  "version": "0.0.1",
  "sources": {
    "dir" : "src",
    "subdirs" : true
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "reason": { "react-jsx": 3 },
  "bs-dependencies": [
    "@rescript/react", 
    "bs-css",
    "bs-css-emotion"
  ],
  "ppx-flags": ["@davesnx/styled-ppx/ppx"],
  "warnings": {
    "error" : "+101"
  }
}
